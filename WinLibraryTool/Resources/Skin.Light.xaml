<ResourceDictionary xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
                    xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml">

  <!-- ======== Light Theme ======== -->

  <Color x:Key="Color.Foreground">#FF1C1C1C</Color>
  <Color x:Key="Color.Background">#FFF5F5F5</Color>

  <Color x:Key="Color.Control.Background">#FFFFFFFF</Color>
  <Color x:Key="Color.Control.Border">#FF8A8A8A</Color>
  <Color x:Key="Color.Control.DisabledForeground">#80FFFFFF</Color>
  <Color x:Key="Color.Control.Hover">#FFE5E5E5</Color>
  <Color x:Key="Color.Control.Pressed">#FFD1D1D1</Color>

  <!-- ======== Resources ======== -->

  <ResourceDictionary.MergedDictionaries>
    <ResourceDictionary Source="pack://application:,,,/WinLibraryTool;component/Resources/Skin.Common.xaml" />
    <ResourceDictionary Source="pack://application:,,,/WinLibraryTool;component/Resources/Icons.xaml" />
  </ResourceDictionary.MergedDictionaries>

</ResourceDictionary>
